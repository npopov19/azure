{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "wip"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "containerpayhist",
						"type": "DatasetReference"
					},
					"name": "payhist"
				},
				{
					"dataset": {
						"referenceName": "containeremployees",
						"type": "DatasetReference"
					},
					"name": "Employees"
				},
				{
					"dataset": {
						"referenceName": "containershifts",
						"type": "DatasetReference"
					},
					"name": "shiftsLookup"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "lookup2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          BusinessEntityID as short,",
				"          RateChangeDate as timestamp 'yyyy-MM-dd',",
				"          Rate as double,",
				"          PayFrequency as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> payhist",
				"source(output(",
				"          BusinessEntityID as short,",
				"          NationalIDNumber as integer,",
				"          LoginID as string,",
				"          OrganizationLevel as short,",
				"          JobTitle as string,",
				"          MaritalStatus as string,",
				"          Shift as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Employees",
				"source(output(",
				"          ShiftID as integer,",
				"          Name as string,",
				"          StartTime as string,",
				"          EndTime as string,",
				"          ModifiedDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> shiftsLookup",
				"lookup2, shiftsLookup lookup(Shift == ShiftID,",
				"     multiple: false,",
				"     pickup: 'first',",
				"     asc(ShiftID, true),",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 derive(ShiftName = Name,",
				"          LoginUserName = substring(LoginID,instr(LoginID,'\\\\')+1,length(LoginID)-instr(LoginID,'\\\\')),",
				"          MartialStatus_Name = case(MaritalStatus=='M','Married','Single'),",
				"          rate = ERROR_FUNCTION(''),",
				"          column1 = ERROR_FUNCTION('')) ~> derivedColumn1",
				"Employees, payhist lookup(Employees@BusinessEntityID == payhist@BusinessEntityID,",
				"     multiple: false,",
				"     pickup: 'first',",
				"     asc(RateChangeDate, true),",
				"     broadcast: 'auto')~> lookup2",
				"derivedColumn1 sink(validateSchema: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          ShiftName,",
				"          LoginUserName,",
				"          MartialStatus_Name",
				"     )) ~> sink1"
			]
		}
	}
}